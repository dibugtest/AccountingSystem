@model IEnumerable<AccountingSystem.Models.rajaswoAsuli>

@{
    ViewBag.Title = "Rajaswo Asuli";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="row">
    <h2 class="text-center">राजश्व असूली विवरण</h2>
    @if (TempData["Message"] != null)
    {
        <div class="alert alert-success">@TempData["Message"]</div>
    }
    @if (TempData["ErrorMessage"] != null)
    {
        <div class="alert alert-danger">@TempData["ErrorMessage"]</div>
    }
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)
        if (ViewBag.ErrorMessage != null)
        { <div class="col-md-12">
            <div class="alert alert-danger text-center">@ViewBag.ErrorMessage</div>
        </div>
        }
        <div class="col-md-4">
            <div class="form-group">
                <label>आ.ब.</label>
                @Html.DropDownList("fyId", null, new { @class = "form-control", id = "ddlFyId" })
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label>महिना</label>
                @Html.DropDownList("month", EnumHelper.GetSelectList(typeof(AccountingSystem.Models.month)), new { @class = "form-control", id = "txtMonth" })
            </div>
        </div>
        <div class="col-md-4 text-center" style="padding-top:20px;"><input type="submit" value="Select" class="btn btn-primary" id="btnSubmit" /></div>
        <br />
    }
    <table class="table table-responsive table-bordered">
        <tr>
            <th>मिति</th>
            <th>रसिद नं.</th>
            <th>बुझाउनेको नाम</th>
            <th>१४१५१ सरकारी सम्पतिको बहाल तथा अन्य आय</th>
            <th colspan="6">१४२२३ शिक्षा क्षेत्रको आम्दानी</th>
            <th>१४२१३ अन्य बिक्रीबाट प्राप्त रकम पुस्तक</th>
            <th>अन्य प्रशासनिक सेवा शुल्क १४२२७ टेण्डर</th>
            <th></th>
        </tr>
        <tr>
            <th></th>
            <th></th>
            <th></th>
            <th></th>
            <th>जन्म मिति</th>
            <th>रि.प्रिन्ट</th>
            <th>माइक्रो फिल्म</th>
            <th>प्रतिलिपि</th>
            <th>भाष्यन्तर व लिप्यन्तर</th>
            <th>डिजिटल इमेज</th>
            <th></th>
            <th></th>
            <th></th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.nepDateStr
                                </td>
                <td>
                    @item.rasidNo
                </td>
                <td>
                    @item.bhujauneKoNaam
                </td>
                <td>
                    @item.kha14151
                </td>
                <td>
                    @item.janmaMiti
                </td>
                <td>
                    @item.rePrint
                </td>
                <td>
                    @item.microFilm
                </td>
                <td>
                    @item.pratilipi
                </td>

                <td>
                    @item.lipyantar
                </td>
                <td>
                    @item.digitalImage
                </td>
                <td>
                    @item.kha14213
                </td>
                <td>
                    @item.kha14227
                </td>
                <td>
                    @Html.ActionLink("Edit", "EditAsuli", new { id = item.raId }) |
                    @Html.ActionLink("Delete", "DeleteAsuli", new { id = item.raId }, new { onclick = "return confirm('"+item.fiscalYear.nepFy +" आ.ब.,"+item.month+ " महिनाको राजश्व असूली विवरण हटाउन चाहनुहुन्छ?');" })
                </td>
            </tr>
        }

    </table>

    <div class="col-md-12 text-center">
        @Html.ActionLink("Create New", "CreateAsuli", null, new { @class = "btn btn-primary" })

    </div>
</div>
@section Scripts{
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jquery.ui")
    @Styles.Render("~/Content/css/jquery.ui")
    <script src="~/Scripts/NepaliUnicode.js"></script>
