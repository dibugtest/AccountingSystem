@{
    ViewBag.Title = "Rajaswo Journal";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}


<div class="row">
    @if (Request.HttpMethod.ToString() == "POST" )
    {
        <h2 class="text-center">राजश्व गोश्वारा भौचरहरु</h2>
        <div class="col-md-2"></div>
        <div class="col-md-4">
            <div class="panel panel-primary">
                <div class="panel-heading text-center">
                    <strong>आम्दानी </strong>
                </div>
                <div class="panel-body">
                    <table class="table table-bordered text-center">
                        <tr>
                            <th class="text-center">
                                वर्ष,महिना
                            </th>
                            <th>गो.भौ.नं.</th>
                            <th></th>
                        </tr>
                        @if (ViewBag.AmdaniJornal != null)
                        { 
                        foreach (var item in ViewBag.AmdaniJornal)
                        {
                            <tr>
                                <td>    
                                    @(item.year + "," + item.month)
                                </td>
                                <td>@item.jornalNo</td>
                                <td>
                                    <a href='@Url.Action("RajaswoJornalBibaran", "Rajaswo", new { id = item.jornalEntriesId })'>
                                        <i class="fa fa-eye" style="font-size:15px;"></i>
                                    </a>
                                </td>
                            </tr>
                        }
                        }
                    </table>
                </div>

            </div>
        </div>
        <div class="col-md-4">
            <div class="panel panel-primary">
                <div class="panel-heading text-center">
                    <strong>दाखिला</strong>
                </div>
                <div class="panel-body">
                    <table class="table table-bordered text-center">
                        <tr>
                            <th class="text-center">
                                वर्ष,महिना
                            </th>
                            <th>गो.भौ.नं.</th>
                            <th></th>
                        </tr>
                        @if (ViewBag.DakhilaJornal != null)
                        { 
                        foreach (var item in ViewBag.DakhilaJornal)
                        {
                            <tr>
                                <td>
                                    @(item.year + "," + item.month)
                                </td>
                                <td>@item.jornalNo</td>
                                <td>
                                    <a href='@Url.Action("RajaswoJornalBibaran", "Rajaswo", new { id = item.jornalEntriesId})'>
                                        <i class="fa fa-eye" style="font-size:15px;"></i>
                                    </a>
                                </td>
                            </tr>
                        }
                        }
                    </table>
                </div>

            </div>
        </div>
        <div class="col-md-2"></div>
    }
    else
    {
        <h2 class="text-center">गोश्वारा भौचरहरुका लागि आर्थिक वर्ष र महिना छान्नुहोस्</h2>
        using (Html.BeginForm())
        {
            <div class="col-md-4"></div>
            <div class="col-md-4">
                <div class="col-md-12">
                    <div class="form-group">
                        <label>आ.ब.</label>
                        @Html.DropDownList("fyId", null, new { @class = "form-control", id = "ddlFyId" })
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="form-group">
                        <label>महिना</label>
                        @Html.DropDownList("mn", EnumHelper.GetSelectList(typeof(AccountingSystem.Models.month)), new { @class = "form-control", id = "txtMonth" })
                    </div>
                </div>
                <div class="col-md-12 text-center">
                    <input type="submit" value="Submit" class="btn btn-primary" id="btnSubmit" />
                </div>
            </div>
            <div class="col-md-4"></div>
        }
    }
</div>




